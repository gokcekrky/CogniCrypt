get-button "Generate Code For Cryptographic Task" | click
with [get-window "Cryptography Task Configurator"] {
    get-combo -after [get-label "Select a Task :"] | select "Securely Archive Files"
    with [get-button "Next >"] {
        click
        click
        click
    }
